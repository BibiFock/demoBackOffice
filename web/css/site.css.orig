/* Text classes */

* .highlight {color:#1A73E9;}
* .middle {vertical-align:center;}
* .top {vertical-align:top;}
* .bottom {vertical-align:bottom;}
* .left {text-align:left;}
* .right {text-align:right;}
* .justify {
    text-align:justify;
    -webkit-hyphens:auto;-moz-hyphens:auto;hyphens:auto;
}
* .center {text-align:center;}
* .hidden {display:none;}
* .arrow:before {content:'\2192';margin:0 1em;font-family:'entypo',sans-serif;font-size:2em;line-height:1;color:#AAA;vertical-align:middle;}

::selection {background:#00BECF;color:#FFF;}
::-moz-selection {background:#00BECF;color:#FFF;}

/* Columns */

.col1 {float:left;width:8.333333333%;}
.p10 {float:left;width:10%;}
.p15 {float:left;width:15%;}

.col2 {float:left;width:16.666666667%;}
.p20 {float:left;width:20%;}

.col3 {float:left;width:24.9%;}
.p30 {float:left;width:30%;}

.col4 {float:left;width:33.333333333%;}
.p40 {float:left;width:40%;}

.col5 {float:left;width:41.666666667%;}
.p50 {float:left;width:50%;}

.col6 {float:left;width:49.9%;}
.p60 {float:left;width:60%;}
.p65 {float:left;width:65%;}

.col7 {float:left;width:56.933333333%;}
.p70 {float:left;width:70%;}

.col8 {float:left;width:66.666666667%;}
.p80 {float:left;width:80%;}
.p85 {float:left;width:85%;}

.col9 {float:left;width:75%;}
.p90 {float:left;width:90%;}

.col10 {float:left;width:83.333333333%;}
.coll11 {float:left;width:91.666666667%;}

/* @clear-fix */
*.clear-fix:after {content:"";display:block;margin:0;padding:0;clear:both;width:100%;height:1px;line-height:1px;}
* .clear{
    clear:both;display:block;float:none;margin:0;padding:0;width:100%;height:0;
    line-height:0;overflow:hidden;background:none;
}

/* Elements */
section, article, header, footer, nav, hgroup, figure, figcaption {display:block;margin:0;padding:0;}
h1, h2, h3, h4, h5, ul, li, form, fieldset {margin:0;padding:0;}
h1, h2, h3, h4, h5 {font-weight:300;color:#00BECF;}

hgroup {margin-bottom:1.5em;}
hgroup h3, hgroup p {margin:0;padding:0;}
hgroup p {color:#333;}

strong, b {font-weight:500;}
i, em {font-weight:300;font-style:oblique;}

a img {border-width:0;}
a:link, a:visited {
	color:#00BECF;text-decoration:none;
	-webkit-transition: color .25s;-moz-transition: color .25s;-o-transition: color .25s;-ms-transition: color .25s;transition: color .25s;
}
a:hover {color:#FCA822;}

pre.debug-data {padding:1%;margin-top:3em;height:600px;overflow:auto;font-size:.75em;font-family:monospace;background-color:#FFFFEA;}

/* @.notify */
.notify, .notify.success {padding:1em;margin-bottom:1.5em;background-color:#ECFAFB;border:1px solid #8FDAE0;font-weight:400;color:#6A9A9E;}
.center .notify {margin: 0 auto 1.5em auto;}
.notify.inline {display:inline-block;}
.notify.error {background-color:#FEF7EF;border:1px solid #FAD3C0;font-weight:400;color:#F6B291;}
.notify .icon:before {margin-right:.25em;font-family:'entypo';font-size:3em;line-height:.333333333;vertical-align:middle;}
.notify.success .icon:before {content:"\1F44D";color:#6A9A9E;}
.notify.error .icon:before {content:"\26A0";color:#F6B291;}
.notify.hidden {display:none;}

/* Form elements */

fieldset {border-width:0;}
.select-wrapper {
	padding:.25em .5em;display:block;background: #FFF url('data:image/png;base64,') no-repeat 100% center;
    border:1px solid #D8DADB;font:normal .875em/1.142857143 sans-serif;
    -webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box;
}
.select-wrapper select {
    display:block;margin:0;padding:0;width:100%;background:none;border:none;
    font:normal 1em/1.142857143 sans-serif;outline:none;
    -webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box;
	-webkit-appearance:none;
}

input[type="text"], input[type="password"], textarea, select { 
    display:block;background-color:#FFF;padding:.25em;width:100%;
    border:1px solid #CCCCCC;font:normal .875em/1.8 sans-serif;outline:none;
    -webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box;
}
input[type="text"]:focus, textarea:focus, input[type="submit"]:focus {background-color:#FFFBDC;}
input[type="text"].inline, input[type="password"].inline, textarea.inline, select.inline {display:inline;width:auto;}
input[disabled="disabled"] {background-color:#FFFBDC;color:#BFBDA5;border-color:#E3E0C4;}

/* Buttons */

label {cursor:pointer;}
input[type="submit"], button, a.btn:link, a.btn:visited {
    padding:.5em .75em;margin:0;border:1px solid #CCC;color:#999;background:#CCC;border-radius:2px;cursor:pointer;
	font:normal 12px/1 'Lato',sans-serif;font-weight:700;letter-spacing:.1em;
    background: #f7f7f7;
	background: -moz-linear-gradient(top,  #fafafa 0%, #f2f2f2 50%, #f2f2f2 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fafafa), color-stop(50%,#f2f2f2), color-stop(100%,#f2f2f2));
	background: -webkit-linear-gradient(top,  #fafafa 0%,#f2f2f2 50%,#f2f2f2 100%);
	background: -o-linear-gradient(top,  #fafafa 0%,#f2f2f2 50%,#f2f2f2 100%);
	background: -ms-linear-gradient(top,  #fafafa 0%,#f2f2f2 50%,#f2f2f2 100%);
	background: linear-gradient(to bottom,  #fafafa 0%,#f2f2f2 50%,#f2f2f2 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fafafa', endColorstr='#f2f2f2',GradientType=0 );
	-webkit-box-shadow:inset 0 1px 0 #FFF;box-shadow:inset 0 1px 0 #FFF;
}
input[type="submit"]:hover, button:hover, a.btn:hover {
	background: #f2f2f2;color:#555555;
	background: -moz-linear-gradient(top,  #f2f2f2 0%, #f2f2f2 50%, #fafafa 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f2f2f2), color-stop(50%,#f2f2f2), color-stop(100%,#fafafa));
	background: -webkit-linear-gradient(top,  #f2f2f2 0%,#f2f2f2 50%,#fafafa 100%);
	background: -o-linear-gradient(top,  #f2f2f2 0%,#f2f2f2 50%,#fafafa 100%);
	background: -ms-linear-gradient(top,  #f2f2f2 0%,#f2f2f2 50%,#fafafa 100%);
	background: linear-gradient(to bottom,  #f2f2f2 0%,#f2f2f2 50%,#fafafa 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f2f2f2', endColorstr='#fafafa',GradientType=0 );
}


/* LAYOUT */

html, body {margin:0 0 0 0;padding:0 0 0 0;}
html {font: 300 100%/1.5 'Lato', sans-serif;}
body {text-align:center;color:#8C8C8C;border-top:4px solid #FFAA00;}

/* @.topnav */
nav.topnav {margin-bottom:1.5em;background-color:#FBFBFB;text-align:center;border-bottom:1px dotted #DDD;}
nav.topnav .wrapper {width:960px;margin: 0 auto;text-align:left;}
nav.topnav p {margin:0;}
nav.topnav img {display:block;margin:0;padding:0;}
nav.topnav .logout {padding:0;margin:0;}
nav.topnav .logout a {font-size:.5625em;line-height:4;color:#CCC;letter-spacing:.2em;}

section.main article {padding-top:1.5em;}
section.main {margin:0 auto;width:960px;text-align:left;}

/* @header.main */
header.main {padding:1.5em 0 0 0;}
header.main h1 {margin-bottom:1.5em;font-size:1em;line-height:1;padding:0;text-align:center;}

header.main hgroup .wrapper {padding:1em 3em 1.5em 0;}
header.main hgroup h2 {margin:.75em 0 0 0;font-size:.875em;font-weight:700;text-transform:uppercase;letter-spacing:.1em;}
header.main hgroup p.editing {position:relative;border-top:1px dotted #DDD;}
header.main hgroup p.editing em {position:absolute;display:block;top:-1em;right:0;bottom:auto;left:0;}
header.main hgroup p.editing em span {padding:0 1em;background-color:#FFF;font-size:.75em;letter-spacing:.1em;}

header.main nav ol {text-align:right;margin:0 0 0 auto;}
header.main nav ol li:first-child p {border-left:1px solid #DDD;}
header.main nav ol li {float:left;width:33.333333333%;margin:0 0 0 0;padding:0 0 0 0;text-align:left;font-size:1.5em;font-weight:400;color:#DDD;}
header.main nav ol li p {margin:0;padding:0 2.5em 0 1.5em;border-right:1px solid #DDD;font-size:.5em;color:#8C8C8C;font-weight:300;letter-spacing:.1em;}
header.main nav ol li a:link, header.main nav ol li a:visited {font-size:1.166666667em;color:#333;text-decoration:none;font-weight:400;}
header.main nav ol li a:hover {color:#111;}
header.main nav ol li a:link.current, header.main nav ol li a:visited.current, header.main nav ol li a:hover.current {color:#33BDCF;}

/* @footer.main */
footer.main {text-align:right;border-top:1px dotted #DDD;}
.copy {font-size:.75em;line-height:2;}

/* @.search */
.search {padding-top:1.5em;padding-left:1.5em;background-color:#FBFBFB;border:1px solid #EEE;border-width:1px 0;}
.search fieldset p {margin:0 1.5em 1.5em 0;}
.search fieldset dl.drop {margin:0 1.5em 1.5em 0;}
.search fieldset dl.drop li {font-size:.875em;line-height:1.142857143;}

/* @.search-select-fields */

/* @.login */
.login {border-top:1px dotted #DDD;}
.login h2 {margin-bottom:1em;}
.login fieldset {margin:3em 15em 6em 15em;padding:1.5em;}

/* @.fields-edit */
.fields-edit {font-size:.875em;}
.fields-edit fieldset {margin-bottom:1.5em;}

.fields-edit h2 .sample {display:block;margin:0;padding:0;color:#555;font-size:.625em;line-height:3;}
.fields-edit h2 .sample:before {content:"\2192";margin-right:.25em;}
.fields-edit .samples ul {list-style-type:square;list-style-position:inside;}
.fields-edit .samples p, .fields-edit .samples li, .fields-edit .samples em {color:#333;font-weight:400;}
.fields-edit .samples strong {font-weight:700;}

.fields-types-container {border-bottom:1px dotted #DDD;padding-bottom:1.5em;margin-bottom:1em;}

.fields-list-value {color:#555;}
.sample-edit {color:#333;font-weight:400;letter-spacing:.1em;}

.fields-edit .column {float:left;width:33%;margin: 0 2% 0 0;}
.fields-edit .column.last {width:63%;margin: 0 0 0 2%;}
.fields-edit nav {margin:0 0 1.5em 0;padding:.75em .125em .75em .25em;border:1px dotted #DDD;border-width:1px 0;}
.fields-edit nav.bottom {margin:1.5em 0 0 0;border-bottom-width:0;}
.fields-edit nav h2 {font-size:1.714285714em;line-height:1;margin-bottom:0;}
.fields-edit nav p {margin:0;}
.fields-edit nav button.prev {border-right-width:0;border-radius:2px 0 0 2px;}
.fields-edit nav button.prev span:before {content:'\E761';font-family:'entypo',sans-serif;font-weight:normal;font-size:2em;line-height:.4;margin-right:.25em;vertical-align:middle;}
.fields-edit nav button.next {border-radius:0 2px 2px 0;}
.fields-edit nav button.next span:after {content:'\E762';font-family:'entypo',sans-serif;font-weight:normal;font-size:2em;line-height:.4;margin-left:.25em;vertical-align:middle;}
.fields-edit nav button.save-field {border-radius:0;border-right-width:0;}
.fields-edit .samples ul {list-style-position:inside;list-style-type:none;}
.fields-edit .samples ul li:before {content:"\E762";font-family:'entypo';margin-right:.5em;font-size:1.5em;}
.fields-edit .raw p {margin-right:1.5em;}

.select-fields ul {list-style-position:inside;list-style-type:none;}
.select-fields li {margin:0;padding:0;cursor:pointer;}
.select-fields span {display:block;padding:.25em 1em;}
.select-fields span.item, .select-fields span.item:before {color:#AAA;}
.select-fields span.item:before {font-family:'entypo';content:"\2796";padding-right:.5em;}
.select-fields span.item:hover {background-color:#F5F5F5;}

.drop span.item.selected, .drop span.item.selected:before {font-weight:400;background-color:#00BECF;color:#FFF;}
.select-fields span.item.selected:after {color:#00BECF;}
.select-fields span.item.selected:before {content:"\2795";padding-right:.5em;font-weight:400;}

/* .select-fields span.item.selected, .select-fields span.item.selected:before {color:#00BECF;font-weight:400;}
.select-fields span.item.selected:after {color:#00BECF;}
.select-fields span.item.selected:before {content:"\2795";padding-right:.5em;font-weight:400;} */

/* @.invite */
.invite{margin-bottom:3em;padding-bottom:3em;border-bottom:1px dotted #DDD;}
.invite a {color:#666;text-decoration:underline;}

/* @.new-pattern */
.pattern-new {margin:0;padding:0;font-size:.875em;}
.pattern-new h2 {font-size:1.714285714em;line-height:1;margin-bottom:1em;}
.pattern.pattern-new {border-bottom-width:0;}
.pattern.pattern-new h3 {padding:0 0 0 0;}
.pattern-new fieldset {padding-top:2em;border-top:1px dotted #DDD;}

/* @.patterns */
.patterns {color:#707070;}
.pattern {margin:0;padding:0;font-size:.875em;border-bottom:1px dotted #DDD;}
.pattern button.inline{ display:inline;}
.pattern.odd {background-color:#F7F7F7;}
.pattern.even {background-color:#FBFBFB;}
.pattern.open {
	background-color:#FFF;
	-webkit-box-shadow: inset 4px 0 0 #FFAA00, inset -4px 0 0 #FFAA00;box-shadow: inset 4px 0 0 #FFAA00, inset -4px 0 0 #FFAA00;
}
.pattern .raw-pattern {margin:0;padding:0 1.714285714em 1.714285714em 1.714285714em;border-top:1px dotted #DDD;border-right:1px dotted #DDD;}
.pattern .tagged-pattern {margin:0;padding:.857142857em;border-right:1px dotted #DDD;}
.pattern h3 {margin:1.5em 0;padding:0 .75em;}
.pattern hgroup h3, .pattern hgroup p {margin:0;padding:0;}
.raw-pattern fieldset p {margin:0 0 .857142857em 0;padding:0;}


/* @.editor */
.editor {
	position:relative;padding:1.142857143em;margin-bottom:1.714285714em;height:12em;border:1px solid #DDD;
}
.pattern-edit-box {padding:0;margin:0;height:200px;font:normal 1em/1.714285714 'Lato',sans-serif;color:#666;outline:none;}
.pattern-edit-box strong {font-size:.785714286em;color:#8F578F;}

.contextual{
	position:absolute;top:-1px;right:-1px;bottom:auto;left:auto;margin:0;padding:0;border:1px solid #DDDDDD;background-color:#FFF;overflow:auto;
	width:0;height:200px;visibility:hidden;
}
.contextual.opened {width:100%;visibility:visible;}

.contextual dl, .contextual dt, .contextual dd {margin:0;padding:0;}
.contextual dt {position:absolute;top:0;right:0;padding:0 1.5em 0 0;background-color:#FFF;}
.contextual dd {-webkit-box-shadow: inset 0 2px 0 #EEE;box-shadow: inset 0 2px 0 #EEE;}
.contextual .close {color:#00BECF;cursor:pointer;border-bottom:1px solid #EEE;}
.contextual .close:before {content:'\2716';font-family:'entypo',sans-serif;font-size:2.5em;line-height:1em;vertical-align:middle;color:#00BECF;cursor:pointer;}
/*.contextual .close:before:hover {color:#FFF;}*/

.contextual ul {margin:0;padding:0;list-style-type:none;text-align:left;}
.contextual ul li {margin:0;padding:.571428571em 1.142857143em;font-size:.75em;letter-spacing:.1em;color:#AAA;cursor:pointer;}
.contextual ul li em {font-size:.916666667em}
.contextual ul li strong {color:#555;}
.contextual ul li:hover {color:#FFF;background-color:#67A4CE;}
.contextual ul li:hover strong {color:#FFF;}
.pattern button {font-size:.875em;}
.pattern button.valid span:before {font-size:3em;line-height:.25;font-family:'entypo';content:"\2713";margin-right:.15em;vertical-align:middle;}
.pattern button.invalid span:before {font-size:3em;line-height:.25;font-family:'entypo';content:"\1F6AB";margin-right:.15em;vertical-align:middle;}

.sentences-list {
	padding:0 1.714285714em;border-top:1px solid #DDD;
	-webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box;
	-webkit-box-shadow:inset 0 2px 0 hsla(1,0%,90%,.25);box-shadow:inset 0 2px 0 hsla(1,0%,90%,.25);
}
.sentences-list .wrapper {overflow:auto;height:400px;}
.sentences-list .wrapper .sentence:last-child {border-bottom-width:0;}
.sentences-list hgroup {margin-top:1.5em;}
.sentences-list hgroup p {padding:0;}
.sentences-list p {margin:0;padding:.857142857em 0 .857142857em .857142857em;}

.sentence {border-bottom:1px dotted #DDD;}
.sentence.odd {background-color:#FBFBFB;}
.sentence.first {border-bottom:1px dotted #DDD;border-top:1px dotted #DDD;}
.sentence:hover {background-color:#FFF9D8;}

.sentence .generated-sentence {padding:1.333333333em .666666667em;font-size:1.142857143em;line-height:1.333333333em;font-weight:400;letter-spacing:.1em;color:#939393;font-style:oblique;}
.sentence .generated-sentence .highlight {color:#707070;font-weight:400;}

.sentences-list .buttons {margin:0;padding:1.714285714em 0;border-top:1px solid #DDDDDD;}

/* @.confirm*/
.confirm {border-top:1px dotted #DDD;}
.confirm h3 {font-size:1.25em;line-height:1.2em;margin-bottom:1.2em;}
.confirm h3.warning {color:#FFAA00;}
.confirm h3.warning:before {content:'\26A0';margin-right:.125em;font-family:'entypo';font-size:2em;line-height:1;vertical-align:middle;color:#FFAA00;}
.confirm ul {margin-bottom:1.5em;list-style-position:inside;list-style-type:none;}
.confirm ul li:before {content:"\E762";font-family:'entypo';margin-right:.5em;font-size:1.5em;}

.confirm .sentences-list {background-color:#FFF;margin:0;padding:0;}
.confirm .sentences-list .sentence.first {border-top-width:0;}
.confirm .sentence {font-size:1.142857143em;}

.confirm.done ul { margin:1.5em auto 3em auto;width:50%;padding:1.5em;border-top:1px dotted #EEE;}

/* @.drop */

dl.drop, .drop dt, .drop dd, .drop ul {display:block;margin:0;padding:0;list-style:none;} 
dl.drop {cursor:pointer;}
.drop {position:relative;}
.drop dt {
	display:block;padding: 0 1.5em 0 .5em;width:100%;height:2em;border:1px solid #DDD;overflow:hidden;
	background: #FFF url('data:image/png;base64,') no-repeat right center;
	line-height:2;font-weight:500;font-size:1em;
	-o-text-overflow: 'ellipsis';-ms-text-overflow: 'ellipsis';text-overflow: 'ellipsis';
	-webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box;
}
.drop dd {
	visibility:hidden;width:0;height:0;opacity:0;overflow:hidden;
	position:absolute;top:2em;right:0;bottom:auto;left:0;background-color:#FFF;border:1px solid #DDD;border-width: 0 1px 1px 1px;
	-webkit-transition: all .5s .5s;-moz-transition: all .5s .5s;-o-transition: all .5s .5s;-ms-transition: all .5s .5s;transition: all .5s .5s;
}
.drop:hover dd {visibility:visible;width:auto;height:auto;opacity:1;overflow:visible;}
.drop li {margin:0;padding:0;}
.drop span {display:block;padding:.25em 1em;}

.drop span.select-all {font-size:.625em;color:#00BECF;}
.drop span.item, .drop span.item:before {color:#AAA;}
.drop span.item:before {font-family:'entypo';content:"\2796";padding-right:.5em;}
.drop span.item:hover {background-color:#F5F5F5;}

.drop span.item.highlight {color:#33BDCF;}
.drop span.item.highlight:before {color:#33BDCF;text-decoration:none;}
.drop span.item.highlight:after {content:"done";margin-left:.5em;color:#33BDCF;text-transform:uppercase;font-size:.5625em;vertical-align:text-top;letter-spacing:.1em;text-decoration:none;}

.drop span.item.highlight.number:after {content:"";}
.drop span.item.highlight.number span{color:#33BDCF;margin-left:.1em;text-transform:uppercase;font-size:.5625em;vertical-align:text-top;letter-spacing:.1em;display:inline;}

.drop span.item.selected, .drop span.item.selected:before {font-weight:400;background-color:#00BECF;color:#FFF;}
.drop span.item.selected:after {color:#397CCD;}
.drop span.item.selected:before {content:"\2795";padding-right:.5em;font-weight:400;}

/* .drop span.item.selected, .drop span.item.selected:before {color:#397CCD;font-weight:400;}
.drop span.item.selected:after {color:#397CCD;}
.drop span.item.selected:before {content:"\2795";padding-right:.5em;font-weight:400;} */

/* @.tag */
span.tag {position:relative;font-size:.875em;}
span.tag, span.tag .caption, span.tag .container {display:inline-block;margin:0;padding:0;list-style-type:none;}
span.tag span.caption {
	padding:0 .5em;background-color:#FFF;border:1px solid #DDD;border-radius:.125em;cursor:pointer;font-size:.857142857em;color:#8C8C8C;font-weight:500;
}
span.tag span.container {
	visibility:hidden;height:0;opacity:0;
	max-height:200px;overflow:auto;
	position:absolute;top:auto;right:auto;bottom:1.5em;left:0;background-color:#FFF;
}
span.tag:hover span.container {
	visibility:visible;height:auto;opacity:1;z-index:12;
	width:200px;border-radius:.125em;border:1px solid #DDD;
	-webkit-transition:all .25s .5s;-moz-transition:all .25s .5s;-o-transition:all .25s .5s;-ms-transition:all .25s .5s;transition:all .25s .5s;
	-webkit-box-shadow: 0 0 10px #C5C5C5;box-shadow: 0 0 10px #C5C5C5;
}
span.tag span.item, span.tag span.list-item {display:block;margin:0;padding:.125em 1em;border-bottom:1px solid #DDD;letter-spacing:.1em;}
span.tag span.item {cursor:pointer;}
span.tag span.item, span.tag span.item:before {color:#AAA;}
span.tag span.item:before {font-family:'entypo';content:"\1F6AB";padding-right:.5em;}
span.tag span.item.selected:before {content:"\2713";padding-right:.5em;}
span.tag span.item.selected, span.tag span.item.selected:before {color:#333;}
span.tag span.item:hover {background-color:#F5F5F5;}
